    shadowOAM[0].attr0 = slime.row | (0xFF & 0) | (0<<13)| (0<<14);
    shadowOAM[0].attr1 = slime.col | (0x1FF & 0) | (1<<14);
    shadowOAM[0].attr2 = ((0)<<12) | ((slime.curFrame * 2)*32+((slime.bouncesRemaining > 0) * 2));
}

