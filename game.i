void initPlayer() {
    slime.col = 1;
    slime.row = 130;
    slime.cdel = 1;
    slime.rdel = 1;
    slime.size = 16;
    slime.bouncesRemaining = 0;
    slime.maxBounces = 4;
    slime.offGround = 0;
    slime.directionFacing = 0;
    livesRemaining = 2;
    slime.curFrame = 0;
    slime.numFrames = 8;
    slime.isWalking = 0;
    slime.aniCounter = 0;
}

void updatePlayer() {

    if (slime.isWalking) {
        slime.isWalking = 0;
    }

    if (collisionCheck(collisionMap, 240, slime.col, slime.row, slime.size, slime.size, 0, slime.rdel)) {
        slime.offGround = 1;
    }
    if ((!(~(oldButtons)&((1<<2))) && (~buttons & ((1<<2))))) {
        cheatsEnabled = (!cheatsEnabled);
    }

    if ((!(~(oldButtons)&((1<<6))) && (~buttons & ((1<<6)))) && !slime.bouncesRemaining) {
        if (!slime.offGround) {
            if (collisionCheck(collisionMap, 240, slime.col, slime.row, slime.size, slime.size, 0, -10 * slime.rdel)) {
                slime.row -= (10 * slime.rdel);
            }
        }
    }


    if ((!(~(oldButtons)&((1<<0))) && (~buttons & ((1<<0))))) {
        if (slime.bouncesRemaining) {
            slime.bouncesRemaining = 0;
            slime.rdel = 1;
            slime.cdel = 1;
        } else if (!slime.offGround) {
            slime.bouncesRemaining = slime.maxBounces;
            slime.rdel = 2;
            slime.cdel = 2;
            slime.cdel += (slime.directionFacing * -4);
        }
    }


    if ((~((*(volatile unsigned short *)0x04000130)) & ((1<<5))) && !slime.bouncesRemaining) {
        if (collisionCheck(collisionMap, 240, slime.col, slime.row, slime.size, slime.size, -(slime.cdel + slime.offGround), 0)) {
            if ((slime.col - slime.cdel) > 0) {
                slime.col -= (slime.cdel + slime.offGround);
            }
            slime.directionFacing = 1;
            if (!slime.offGround) {
                slime.isWalking = 1;
            }
        }
    }


    if ((~((*(volatile unsigned short *)0x04000130)) & ((1<<4))) && !slime.bouncesRemaining) {
        if (collisionCheck(collisionMap, 240, slime.col, slime.row, slime.size, slime.size, (slime.cdel + slime.offGround), 0)) {
            if ((slime.col + slime.cdel) < (240 - slime.size)) {
                slime.col += (slime.cdel + slime.offGround);
            }
            slime.directionFacing = 0;
            if (!slime.offGround) {
                slime.isWalking = 1;
            }
        }
    }



    if (slime.offGround && !slime.bouncesRemaining) {
        slime.row += slime.rdel;
        if (!collisionCheck(collisionMap, 240, slime.col, slime.row, slime.size, slime.size, 0, slime.rdel)) {
            slime.offGround = 0;
        }
    }


    if (slime.bouncesRemaining) {


        if (!collisionCheck(collisionMap, 240, slime.col, slime.row, slime.size, slime.size, slime.cdel, slime.rdel)) {
            if (!cheatsEnabled) {
                slime.bouncesRemaining--;
            }
            playSoundB(bounceSFX_data, bounceSFX_length, 0);
            if (!collisionCheck(collisionMap, 240, slime.col, slime.row, slime.size, slime.size, 0, slime.rdel)) {
                slime.rdel *= -1;
            }
            if (!collisionCheck(collisionMap, 240, slime.col, slime.row, slime.size, slime.size, slime.cdel, 0)) {
                slime.cdel *= -1;
            }
        }

        slime.row += slime.rdel;
        slime.col += slime.cdel;

        if (!slime.bouncesRemaining) {
            slime.rdel = 1;
            slime.cdel = 1;
        }
    }

    animatePlayer();
}

void animatePlayer() {
    if (slime.aniCounter % 5 == 0) {
        slime.curFrame = (slime.curFrame + 1 ) % (slime.numFrames);
    }
    if (slime.isWalking) {
        slime.aniCounter++;

    } else {
        slime.curFrame = 0;
    }
}

